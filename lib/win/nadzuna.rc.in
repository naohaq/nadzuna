#include "winresrc.h"

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @Nadzuna_VERSION_MAJOR@,@Nadzuna_VERSION_MINOR@,0,0
 PRODUCTVERSION @Nadzuna_VERSION_MAJOR@,@Nadzuna_VERSION_MINOR@,0,0
