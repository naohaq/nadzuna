            VALUE "CompanyName", "The Nadzuna Project"
            VALUE "FileDescription", "Nadzuna API DLL"
            VALUE "FileVersion", "@Nadzuna_VERSION_MAJOR@,@Nadzuna_VERSION_MINOR@,0,0"
            VALUE "ProductVersion", "@Nadzuna_VERSION_MAJOR@,@Nadzuna_VERSION_MINOR@,0,0"
            VALUE "ProductName", "@CMAKE_PROJECT_NAME@"
            VALUE "InternalName", "nadzuna"
            VALUE "LegalCopyright", L"Copyright \xA9 @COPYRIGHT_YEAR@ The Nadzuna Project"
            VALUE "OriginalFilename", "libnadzuna.dll"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
